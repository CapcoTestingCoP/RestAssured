{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "url": "/whatever"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "text/plain",
          "Set-Cookie": [
            "session_id=91837492837",
            "split_test_group=B"
          ],
          "Cache-Control": "no-cache"
        }
      }
    },
    {
      "priority": 10,
      "request": {
        "method": "ANY",
        "urlPattern": ".*"
      },
      "response": {
        "status": 404,
        "jsonBody": {
          "status": "Error",
          "message": "Endpoint not found"
        },
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "POST",
        "urlPath": "/users",
        "bodyPatterns": [
          {
            "matchesJsonPath": "$[?(@.name)]"
          }
        ]
      },
      "response": {
        "status": 201,
        "body": "{ \"id\": 2, \"name\": \"Jane Doe\" }",
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "PUT",
        "urlPathPattern": "/users/[0-9]+",
        "queryParameters": {
          "active": {
            "matches": "true"
          }
        }
      },
      "response": {
        "status": 200,
        "body": "{ \"message\": \"User updated successfully\" }",
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    {
      "request": {
        "method": "DELETE",
        "urlPathPattern": "/users/[0-9]+"
      },
      "response": {
        "status": 204
      }
    },
    {
      "request": {
        "method": "GET",
        "urlPath": "/users/1"
      },
      "response": {
        "status": 200,
        "body": "{ \"id\": 1, \"name\": \"John Doe\" }",
        "headers": {
          "Content-Type": "application/json"
        }
      }
    }
  ]
}
